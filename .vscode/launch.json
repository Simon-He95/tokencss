{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceRoot}/packages/vscode"],
      "outFiles": ["${workspaceRoot}/packages/vscode/out/extension.js"]
    }
  ]
}
